{
  "manifest_version": 2,

  "name": "Meetup Batch-Event-Set Tool",
  "short_name": "MBEST",
  "description": "This extension allows you to schedule multiple Meetup.com events at once",
  "version": "5.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "meetup-icon-white-black.png",
    "48": "meetup-logo-small.png",
    "128": "meetup-logo-small.png"
  },
  "background": {
    "scripts": [
      "app/bgscript-constants.js",
      "app/bgscript-utils.js",
      "app/bgscript-xhrs.js",
      "app/bgscript-main.js"
    ]
  },
  "permissions": [
    "storage",
    "identity",
    "tabs",
    "activeTab",
    "http://*.meetup.com/*"
  ],
  "content_scripts": [
    {
      "js": ["app/content.js"],
      "matches": ["https://*.meetup.com/*"]
    }
  ],
  "web_accessible_resources" : ["/oauth2/*"],
  "oauth2": {
    "client_id": "466748401928-m88okvel4gdsc9rjo9qlo8em25ihs65s.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  }
}


