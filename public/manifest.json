{
  "manifest_version": 2,

  "name": "My Extension",
  "description": "This extension is a starting point to create a real Chrome extension",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "icons": {
    "16": "meetup-icon-white.png",
    "48": "meetup-logo-small.png",
    "128": "meetup-logo-small.png"
  },
  "background": {
    "scripts": ["app/background.js"]
  },
  "permissions": [
    "storage",
    "identity",
    "tabs",
    "activeTab",
    "webRequest",
    "http://*.meetup.com/*"
  ],
  "content_scripts": [
    {
      "js": ["app/jquery-3.3.1.min.js", "app/content.js"],
      // "matches": ["http://*.meetup.com/*"]
      "matches": ["<all_urls>"]
    }
  ]
}